{
  "dashboard": {
    "id": null,
    "title": "Fibonacci Service Monitoring",
    "tags": ["fibonacci", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Requests",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{path}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 2,
        "title": "Calculation Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(fibonacci_calculation_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Redis Cache Hits/Misses",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(redis_cache_total{type=\"hit\"}[5m])",
            "legendFormat": "Hits",
            "refId": "A"
          },
          {
            "expr": "rate(redis_cache_total{type=\"miss\"}[5m])",
            "legendFormat": "Misses",
            "refId": "B"
          }
        ]
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(http_errors_total[5m])",
            "legendFormat": "Errors",
            "refId": "A"
          }
        ]
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    }
  },
  "overwrite": true
}
